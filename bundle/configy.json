{
    "version": "v1",
    "cmd": "/bin/bash",
    "isolation": {
         "cgroups": [
            {
                "type": "cpu",
                "resource": "cpu.max",
                "value": "10000 100000"
            },
            {   
                "type": "memory",
                "resource": "memory.max",
				"value": "512M"
			},
			{   
                "type": "pids",
                "resource": "pids.max",
				"value": "10"
			}
        ],
        "namespaces": [
            "pid",
			"network",
			"ipc",
			"uts",
			"mount",
			"cgroup"
        ]
    },
    "userns": {
        "user": {
            "uid": 0,
            "gid": 0
        },
        "uidMappings":  {
            "containerID": 0,
            "hostID": 1000,
            "size": 1000
        },
        "gidMappings": {
            "containerID": 0,
            "hostID": 1000,
            "size": 1000
         }
        
    }
}